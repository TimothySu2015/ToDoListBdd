{
  "name": "todolist-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:bdd": "cucumber-js",
    "test:bdd:html": "cucumber-js --format html:cucumber-report.html",
    "test:bdd:watch": "cucumber-js --watch",
    "test:all": "npm run test && npm run test:bdd",
    "lint": "ng lint"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.0",
    "@angular/cli": "^20.1.0",
    "@angular/compiler-cli": "^20.1.0",
    "@cucumber/cucumber": "^12.1.0",
    "@cucumber/html-formatter": "^21.14.0",
    "@types/chai": "^4.3.20",
    "@types/chai-spies": "^1.0.6",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.1.1",
    "chai": "^4.5.0",
    "chai-spies": "^1.1.0",
    "eslint": "^9.29.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.34.1"
  }
}
